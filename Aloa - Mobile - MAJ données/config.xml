  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2.60">
    <script>#!/usr/bin/env groovy
pipeline {
    agent any
    stages {
        stage(&apos;Récupération du CSV&apos;) {
            steps {
                sh &apos;&apos;&apos;#!/bin/bash
                cd /var/www/html/AloaMobile/aloaMobile;
                if ssh aloaovhjok@ftp.cluster023.hosting.ovh.net stat /homez.105/aloaovhjok/.pureftpd-upload* &gt; /dev/null 2&gt;&amp;1
                    then
                        echo &quot;Le fichier est en cours de transfert&quot;
                        false
                    else
                        echo &quot;Le fichier n&apos;est plus en cours de transfert&quot;
                        sudo scp -i /var/lib/jenkins/.ssh/id_rsa aloaovhjok@ftp.cluster023.hosting.ovh.net:/homez.105/aloaovhjok/A-SuiviTéléphone.csv . 
                        sudo chown aloa:aloa A-SuiviTéléphone.csv
                fi
                &apos;&apos;&apos;
            }
        }
        stage(&apos;Import des données&apos;) {
            steps {
                sh &apos;&apos;&apos;#!/bin/bash
                cd /var/www/html/AloaMobile/aloaMobile;
                php bin/console app:importCsv:suiviTelephone -v;
                &apos;&apos;&apos;
            }
        }
    }
}</script>
